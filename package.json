{
  "name": "css3-h5-api",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "eslint": "eslint --config .eslintrc.js src/js/**/*.js",
    "start": "concurrently --kill-others \"npm run babel-watch\" \"npm run server\"",
    "dev": "concurrently --kill-others \"npm run babel-watch\" \"npm run server\"",
    "debug": "concurrently --kill-others \"npm run babel-watch\" \"node --inspect scripts/server.js\"",
    "debug2": "node --inspect scripts/server-2.js",
    "server-2": "node scripts/server-2.js",
    "babel": "babel src --out-dir dist --source-maps inline",
    "babel-watch": "babel src --out-dir dist --watch",
    "server": "node scripts/server.js",
    "http2": "node --inspect ./scripts/server-http2-native.js",
    "http2-express": "node ./scripts/server-http2-express.js",
    "rollup:build": "cross-env NODE_ENV=\"production\" rollup --config rollup.config.js",
    "rollup:watch": "rollup --config rollup.config.js --watch",
    "sdk:dev": "concurrently --kill-others \"npm run rollup:watch\" \"npm run server\""
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "babel-polyfill": "^6.26.0",
    "babel-standalone": "^6.26.0",
    "chalk": "^2.3.0",
    "compress": "^0.99.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express": "^5.0.0-alpha.7",
    "formidable": "^1.2.2",
    "ip": "^1.1.5",
    "log4js": "^3.0.5",
    "multer": "^1.4.2",
    "node-websocket-server": "^1.1.4",
    "open": "0.0.5",
    "ora": "1.3.0",
    "rimraf": "2.6.2",
    "socket.io": "^2.3.0",
    "spdy": "^4.0.0",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "8.0.1",
    "bufferutil": "^4.0.1",
    "concurrently": "^3.5.1",
    "cross-env": "^6.0.3",
    "eslint": "4.10.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "event-source-polyfill": "0.0.12",
    "hex64": "^0.4.0",
    "rollup": "^1.24.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.3",
    "utf-8-validate": "^5.0.2",
    "uglifyjs": "^2.4.11"
  },
  "nodejs": {
    "version": ">=8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appletxm/css3-h5-api"
  },
  "keywords": [
    "css3",
    "h5"
  ],
  "author": "appletxm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appletxm/css3-h5-api/issues"
  }
}
